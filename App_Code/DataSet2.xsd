                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        sales_entry.invoice_no, sales_entry.date, sales_entry.customer_name, sales_entry.customer_Address, sales_entry.Mobile_no, sales_entry.staff_name, 
                         sales_entry.total_qty, sales_entry.total_amount, sales_entry.grand_total, sales_entry.paid_amount, sales_entry.Pending_amount, sales_entry.status, 
                         sales_entry.value, sales_entry_details.invoice_no AS Expr1, sales_entry_details.barcode, sales_entry_details.Product_code, sales_entry_details.product_name, 
                         sales_entry_details.mrp, sales_entry_details.size, sales_entry_details.color, sales_entry_details.qty, sales_entry_details.dis_per, sales_entry_details.dis_amount,
